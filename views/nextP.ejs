
<%- include("allLinks.ejs"); %>

<body>

    <% resp %>

        <section>

            <% if(resp === 404) { %>
                <h1>Please Enter Valid or Correct Spelling</h1>
                <a href="/" style="font-size: 1.5rem;">Go Back to Home</a>
            <% }else{ %>
            
            <div class="row" id="main">
                <div class="col-lg-4 col-sm-12 div1">
                    <form class="formData" action="/next" method="post">
                        <input type="text" placeholder="" id="cityNameNxtP" name="cityNameP2">
                        <button class="searchBtn">
                            <i class="fa-solid fa-magnifying-glass fa-xl">

                            </i></button>
                    </form>

                    <div id="div1Detail">
                        <div style="text-align: center;">
                            <i style="display: inline; " class="fa-sharp fa-solid fa-location-dot fa-lg"></i>
                            <h4 style="display: inline; " class="cityVal">
                                <%= location %>,<%= countryCode %>
                            </h4>
                        </div>
                        <div>
                            <img class="img" src="<%=src%>" style="width: 200px; height: 200px;" srcset="">
                            <p style="text-align: center;">
                                <%=clouds%>
                            </p>
                        </div>
                        <div style="text-align: center;">
                            <h3 class="tempC" style="display: inline;">
                                <%= temp %><span>&#8451; </span>
                            </h3>
                            <span><i class="fa-solid fa-grip-lines-vertical fa-2xl"></i></span>
                            <h3 style="display: inline;">
                                <%= tempF %> &#8457;
                            </h3>
                        </div>

                        <p class="feelsLike" style="color: gray; text-align: center;">Feel's like <%= tFeels %>
                        </p>

                        <hr>
                        <div class="footer">
                            <h6>Made with <i class="fa-sharp fa-regular fa-heart fa-lg" style="color: #f51414;"></i> by
                                Harsh!</h6>
                            <p style="color: rgb(37, 36, 36); text-align: center;">
                                <%= todayDate %>,<%= todayTime %>
                            </p>
                        </div>
                    </div>
                </div>

                <div class="col-lg-8 col-sm-12 div2" id="details">
                    <div class="row" id="beforeBasic">
                        <div class="col col-lg-5 col-sm-8" id="basic">
                            <p>Humidity</p>
                            <div class="contImg">
                                <img class="someIcons"
                                    src="https://media.istockphoto.com/id/1277328164/vector/humidity-icon.jpg?s=612x612&w=0&k=20&c=na3bVWCUFazxveitRMDgxJUi-mr1LOUG1NBzq_XDFw4="
                                    alt="">
                                <h4 class="Val">
                                    <%= humidity %>%
                                </h4>
                            </div>
                        </div>
                        <div class="col col-lg-5 col-sm-8" id="basic">
                            <p>Pressure</p>
                            <div class="contImg">
                                <img class="someIcons" src="https://cdn-icons-png.flaticon.com/512/1839/1839341.png"
                                    alt="">
                                <h4 class="Val">
                                    <%= pressure %>
                                </h4>
                            </div>
                        </div>
                    </div>

                    <div class="row" id="beforeBasic">
                        <div class="col col-lg-5 col-sm-8" id="basic">
                            <p>Temperature</p>
                            <div class="contImg">
                                <img class="MaxTemp"
                                    src="data:image/jpeg;base64,">
                                <div class="temp">
                                    <p class="tempVal">Max: <%= tempMinimum %>
                                    </p>
                                    <p class="tempVal">Min: <%= tempMinimum %>
                                    </p>
                                </div>

                            </div>
                        </div>
                        <div class="col col-lg-5 col-sm-8" id="basic">
                            <p>Wind</p>
                            <div class="contImg">
                                <img class="someIcons"
                                    src="https://png.pngtree.com/png-clipart/20190925/original/pngtree-wind-icon-for-your-project-png-image_4892341.jpg"
                                    alt="">

                                <div class="temp">
                                    <p class="tempVal">Speed: <%= windSpeed %>km/h</p>
                                    <p class="tempVal">Deg: <%= windDegree %>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

            </div>

            <% } %>
        </section>

</body>

</html>